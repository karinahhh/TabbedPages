<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TabbedPages.Page2"
             Title="Prantsusmaa">
     <ContentPage.Content>
         <StackLayout>
            <Frame x:Name="prants" BackgroundColor="#F5F5DC">
                <Label Text="Pont-Neuf"  
                    VerticalOptions="CenterAndExpand" 
                    HorizontalOptions="CenterAndExpand"
                       FontSize="Large"/>
            </Frame>
            <Frame x:Name="prants2" BackgroundColor="#FFE4C4">
                <Label Text="Eiffeli torn"  
                    VerticalOptions="CenterAndExpand" 
                    HorizontalOptions="CenterAndExpand"
                       FontSize="Large"/>
            </Frame>
            <Frame x:Name="prants3" BackgroundColor="#F5DEB3">
                <Label Text="Arc de Triomphe"  
                       VerticalOptions="CenterAndExpand" 
                       HorizontalOptions="CenterAndExpand"
                       FontSize="Large"/>
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>