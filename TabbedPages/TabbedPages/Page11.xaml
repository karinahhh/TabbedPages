<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TabbedPages.Page11"
             Title="Indoneesia">
    <ContentPage.Content>
        <StackLayout>
            <Frame x:Name="ind" BackgroundColor="#d65cad">
                <Label Text="Warung"  
                    VerticalOptions="CenterAndExpand" 
                    HorizontalOptions="CenterAndExpand"
                       FontSize="Large"/>
                </Frame>
            <Frame x:Name="ind2" BackgroundColor="#cc3399">
                <Label Text="Borobudur"  
                    VerticalOptions="CenterAndExpand" 
                    HorizontalOptions="CenterAndExpand"
                       FontSize="Large"/>
                </Frame>
            <Frame x:Name="ind3" BackgroundColor="#8f246b">
                <Label Text="Ijeni kraater"  
                       VerticalOptions="CenterAndExpand" 
                       HorizontalOptions="CenterAndExpand"
                       FontSize="Large"/>
                </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>